<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="450.0" prefWidth="709.0" style="-fx-background-color: #675E5B;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="productTBV" editable="true" prefHeight="450.0" prefWidth="570.0" style="-fx-background-color: #827571;" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="nameCol" minWidth="-Infinity" prefWidth="85.0" style="-fx-background-color: #5c524f; -fx-border-color: black;" text="Producto" />
                <TableColumn fx:id="enabledCol" minWidth="-Infinity" prefWidth="93.0" style="-fx-background-color: #5c524f; -fx-border-color: black;" text="Habilitado" />
                  <TableColumn fx:id="ingredientsCol" minWidth="-Infinity" prefWidth="85.0" style="-fx-background-color: #5c524f; -fx-border-color: black;" text="Ingredientes" />
                  <TableColumn fx:id="sPriceCol" minWidth="-Infinity" prefWidth="93.0" style="-fx-background-color: #5c524f; -fx-border-color: black;" text="Precio S" />
                  <TableColumn fx:id="mPriceCol" minWidth="-Infinity" prefWidth="117.0" style="-fx-background-color: #5c524f; -fx-border-color: black;" text="Precio M" />
                  <TableColumn fx:id="lPriceCol" minWidth="-Infinity" prefWidth="117.0" style="-fx-background-color: #5c524f; -fx-border-color: black;" text="Precio L" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <VBox alignment="CENTER" maxWidth="-Infinity" prefHeight="450.0" prefWidth="203.0" spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button mnemonicParsing="false" text="Import" />
                  <Button mnemonicParsing="false" text="Export" />
                  <Label fx:id="spacer2" prefHeight="65.0" prefWidth="0.0" />
                  <Button mnemonicParsing="false" text="Add" />
                  <Button mnemonicParsing="false" text="Delete" />
                  <Label fx:id="spacer3" />
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" VBox.vgrow="NEVER">
                     <children>
                        <Label text="Last edited by: " />
                        <Label fx:id="lastUserLBL" text="root" />
                     </children>
                  </HBox>
               </children></VBox>
         </children>
      </HBox>
   </center>
</BorderPane>
